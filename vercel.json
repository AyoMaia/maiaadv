{
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/$1" },
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/$1" }
    ],

    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "NODE_VERSION": "20.x"
    }
  }